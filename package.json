{
  "name": "venorus",
  "version": "1.0.0",
  "description": "Platform for promoting Russian products with advanced cataloging and administrative capabilities",
  "private": false,
  "author": {
    "name": "Venorus Team",
    "email": "support@venorus.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iMAGRAY/venorus.git"
  },
  "homepage": "https://github.com/iMAGRAY/venorus#readme",
  "bugs": {
    "url": "https://github.com/iMAGRAY/venorus/issues"
  },
  "keywords": [
    "russian",
    "products",
    "catalog",
    "nextjs",
    "typescript",
    "postgresql",
    "redis",
    "admin-panel",
    "ecommerce",
    "inventory-management"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "node scripts/kill-port.js 3010 && next dev -p 3010",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" next build --no-lint",
    "build:win": "set NODE_OPTIONS=--max-old-space-size=8192 && next build --no-lint",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:auto-fix": "node scripts/tools/auto-fix-lint.js",
    "lint:auto-fix:hooks": "node scripts/tools/auto-fix-hooks-deps.js",
    "lint:auto-fix:deps": "node scripts/tools/auto-fix-callback-deps.js",
    "lint:auto-fix:keys": "node scripts/tools/auto-fix-jsx-keys.js",
    "lint:auto-fix:all": "node scripts/tools/auto-fix-lint.js && node scripts/tools/auto-fix-hooks-deps.js && node scripts/tools/auto-fix-jsx-keys.js && node scripts/tools/auto-fix-callback-deps.js && node scripts/tools/auto-fix-shorthand-alias.js",
    "lint:auto-fix:shorthand": "node scripts/tools/auto-fix-shorthand-alias.js",
    "lint:auto-fix:shorthand:tsc": "node scripts/tools/fix-shorthand-from-tsc.js",
    "type-check": "tsc --noEmit",
    "test": "node tests/run-all-tests.js",
    "test:watch": "node tests/run-all-tests.js",
    "test:smoke": "node tests/smoke-tests.js",
    "test:integration": "node tests/integration/api-integration-tests.js",
    "test:e2e": "node tests/e2e/critical-user-flows.js",
    "test:security": "node tests/security/security-tests.js",
    "test:performance": "node tests/performance/load-tests.js",
    "test:quick": "node tests/run-all-tests.js --quick",
    "test:ci": "node tests/run-all-tests.js --ci",
    "test:coverage": "NODE_V8_COVERAGE=.coverage node tests/run-all-tests.js",
    "test:perf:db": "node tests/performance/db-latency.test.js",
    "db:migrate": "node scripts/migration/run-migration.js",
    "db:seed": "node scripts/setup/seed-data.js",
    "db:reset": "node scripts/setup/init-db.js",
    "db:status": "node scripts/analysis/check-database-state.js",
    "db:backup": "node scripts/database/backup-full.js",
    "db:audit": "node scripts/analysis/db-audit.js",
    "db:audit:out": "mkdir -p database/reports && node scripts/analysis/db-audit.js",
    "redis:test": "node scripts/test-redis-connection.js",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next && npm run build",
    "analyze": "cross-env ANALYZE=true npm run build",
    "security:check": "node scripts/analysis/security-audit.js",
    "db:apply-indexes": "node scripts/analysis/apply-indexes.js",
    "db:migrate:ensure": "node scripts/migration/ensure-missing-tables.js",
    "db:optimize": "npm run db:audit:out && npm run db:apply-indexes && npm run test:perf:db",
    "analyze:deps": "npx depcheck",
    "analyze:types": "npx ts-prune",
    "deploy:auto": "node scripts/deploy/auto-deploy.js",
    "deploy:manual": "npm run build && npm run start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.828.0",
    "@aws-sdk/s3-request-presigner": "^3.828.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-aspect-ratio": "1.1.1",
    "@radix-ui/react-avatar": "1.1.2",
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-collapsible": "1.1.2",
    "@radix-ui/react-context-menu": "2.2.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-hover-card": "1.1.4",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-menubar": "1.1.4",
    "@radix-ui/react-navigation-menu": "1.2.3",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-progress": "1.1.1",
    "@radix-ui/react-radio-group": "1.2.2",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "1.1.1",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "1.2.4",
    "@radix-ui/react-toggle": "1.1.1",
    "@radix-ui/react-toggle-group": "1.1.1",
    "@radix-ui/react-tooltip": "1.1.6",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "commander": "^14.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "embla-carousel-react": "8.5.1",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.23.6",
    "immer": "10.1.1",
    "input-otp": "1.4.1",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.454.0",
    "next": "^15.4.6",
    "next-themes": "^0.4.4",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.1",
    "react-resizable-panels": "^2.1.7",
    "react-window": "^1.8.11",
    "recharts": "2.15.0",
    "redis": "^5.5.6",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "use-sync-external-store": "1.2.2",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^22.16.3",
    "@types/pg": "^8.15.4",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-window": "^1.8.8",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.30",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "ts-morph": "^26.0.0",
    "ts-prune": "0.10.0",
    "typescript": "^5.8.3"
  }
}
